[ ca ]
default_ca = CA_default

[ CA_default ]
dir = ./demoCA
database = $dir/index.txt
new_certs_dir = $dir/newcerts
certificate = ./ca.crt
serial = $dir/serial
clrnumber = $dir/crlnumber
private_key = ./ca.key
default_days = 365
default_crl_days = 30
default_md = sha256
policy = policy_anything

[ policy_anything ]
commonName = supplied
